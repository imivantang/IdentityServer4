msgid "``grant_type``"
msgstr ""

#: ../endpoints/token.rst:15
msgid ""
"``authorization_code``, ``client_credentials``, ``password``, "
"``refresh_token``, ``urn:ietf:params:oauth:grant-type:device_code`` or "
"custom"
msgstr ""

